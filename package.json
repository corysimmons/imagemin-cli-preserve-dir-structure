{
  "name": "imagemin-cli-preserve-dir-structure",
  "author": "Cory Simmons",
  "description": "imagemin-cli has recurring problems preserving dir structure. This is a bandage.",
  "version": "0.0.2",
  "main": "index.js",
  "bin": {
    "imagemin-dir": "index.js"
  },
  "js:next": "lib/index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "scripts": {
    "start": "babel -w lib/index.js -o index.js"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "fs-extra": "^4.0.3",
    "glob": "^7.1.1",
    "imagemin-cli": "^3.0.0",
    "is-image": "^2.0.0",
    "meow": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^4.13.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "shelljs": "^0.7.5"
  }
}
